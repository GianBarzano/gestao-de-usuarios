{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Docker: Attach to Node Back",
      "port": 9229,
      "address": "localhost",
      "localRoot": "${workspaceFolder}/back/src/",
      "sourceMapPathOverrides": {
        "/usr/app/*": "${workspaceRoot}/back/*"
      },
      "remoteRoot": "/usr/app/src/",
      "protocol": "inspector",
      "restart": true
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Docker: Attach to Node Back 2",
      "port": 9229,
      "address": "localhost",
      "localRoot": "${workspaceFolder}/back2/src/",
      "sourceMapPathOverrides": {
        "/usr/app/*": "${workspaceRoot}/back2/*"
      },
      "remoteRoot": "/usr/app/src/",
      "protocol": "inspector",
      "restart": true
    }
  ]
}